<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
    
    <managed-bean>
		<managed-bean-name>controllerServicos</managed-bean-name>
		<managed-bean-class>controller.ControllerServicos</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>menuBean</managed-bean-name>
		<managed-bean-class>model.MenuBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>sindaSolicitacao</managed-bean-name>
		<managed-bean-class>model.Solicitacao</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>controllerSession</managed-bean-name>
		<managed-bean-class>controller.ControllerSession</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<navigation-rule>
		<display-name>ManterUsuario</display-name>
		<from-view-id>/ManterUsuario.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Servicos</from-outcome>
			<to-view-id>/servicos.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Editar</from-outcome>
			<to-view-id>/Edicao.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Visualizar</from-outcome>
			<to-view-id>/Visualizacao.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Buscar</from-outcome>
			<to-view-id>/busca.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>OperacaoSucesso</display-name>
		<from-view-id>/OperacaoSucesso.jsp</from-view-id>
		<navigation-case>
			<from-outcome>manterUsuario</from-outcome>
			<to-view-id>/ManterUsuario.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>OperacaoFail</display-name>
		<from-view-id>/OperacaoFail.jsp</from-view-id>
		<navigation-case>
			<from-outcome>manterUsuario</from-outcome>
			<to-view-id>/ManterUsuario.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>servicos</display-name>
		<from-view-id>/servicos.jsp</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/OperacaoSucesso.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>erro</from-outcome>
			<to-view-id>/OperacaoFail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>Edicao</display-name>
		<from-view-id>/Edicao.jsp</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/OperacaoSucesso.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>erro</from-outcome>
			<to-view-id>/OperacaoFail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>BuscaPorPais</display-name>
		<from-view-id>/buscaPorPais.jsp</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/OperacaoSucesso.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>erro</from-outcome>
			<to-view-id>/OperacaoFail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>BuscaPorNome</display-name>
		<from-view-id>/buscaPorNome.jsp</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/OperacaoSucesso.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>erro</from-outcome>
			<to-view-id>/OperacaoFail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>Busca</display-name>
		<from-view-id>/busca.jsp</from-view-id>
		<navigation-case>
			<from-outcome>BuscarPorNome</from-outcome>
			<to-view-id>/buscaPorNome.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>BuscarPorPais</from-outcome>
			<to-view-id>/buscaPorPais.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>


</faces-config>
